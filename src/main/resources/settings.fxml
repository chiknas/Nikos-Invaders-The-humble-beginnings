           fx:controller="com.chiknas.application.settings.Settings">
    <Group StackPane.alignment="TOP_LEFT">
        <Label fx:id="backButton" layoutX="63.0" layoutY="56.0" styleClass="settings-label"/>
        <StackPane.margin>
            <Insets left="50.0" top="50.0"/>
        </StackPane.margin>
    </Group>

    <Group StackPane.alignment="CENTER">
        <StackPane.margin>
            <Insets left="25.0"/>
        </StackPane.margin>
        <RadioButton fx:id="easy" layoutX="338.0" layoutY="190.0" mnemonicParsing="false"
                     styleClass="settings-label" text="Easy" textFill="WHITE"/>
        <RadioButton fx:id="normal" layoutX="538.0" layoutY="190.0" mnemonicParsing="false"
                     styleClass="settings-label" text="Normal" textFill="WHITE"/>
        <Label layoutX="555.0" layoutY="101.0" styleClass="settings-label" text="Select difficulty"
               textFill="WHITE"/>
        <RadioButton fx:id="hard" layoutX="738.0" layoutY="190.0" mnemonicParsing="false"
                     styleClass="settings-label" text="Impossibru!" textFill="WHITE"/>
        <GridPane fx:id="characters" gridLinesVisible="true" layoutX="323.0" layoutY="437.0" prefHeight="322.0"
                  prefWidth="307.0" style="-fx-background-color: #DEB887;">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <effect>
                <InnerShadow choke="0.37" height="71.41" radius="34.4625" width="68.44"/>
            </effect>
        </GridPane>
        <GridPane fx:id="bullets" gridLinesVisible="true" layoutX="683.0" layoutY="437.0" prefHeight="322.0"
                  prefWidth="307.0" style="-fx-background-color: #DEB887;">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <effect>
                <InnerShadow choke="0.44" height="71.41" radius="34.4625" width="68.44"/>
            </effect>
        </GridPane>

        <Label layoutX="323.0" layoutY="366.0" styleClass="settings-label" text="Player: " textFill="WHITE"/>
        <Label layoutX="683.0" layoutY="366.0" styleClass="settings-label" text="Weapon:" textFill="WHITE"/>
        <Label fx:id="playerName" layoutX="413.0" layoutY="366.0" styleClass="settings-label" textFill="WHITE"/>
        <Label fx:id="weaponName" layoutX="793.0" layoutY="366.0" styleClass="settings-label" textFill="WHITE"/>
    </Group>
